section#section-framed
  h1.section-title Our Work  
  ul.work-tabbed-nav
    li(data-nav="murals", data-context="tab").work-tab.tab-murals Interior Murals
    li(data-nav="fine-art", data-context="tab").work-tab.tab-fine-art Fine/Portrait Art
    li(data-nav="framed", data-context="tab").work-tab.is-active.tab-framed Framed Art
  ul.work-list
    li.work-item
      a.desc(href="img/work-framed-full-01.jpg",rel="group")
        .title Mural of a Lake Somewhere
        .type Oil
        .dimension 24x24
      img(src='img/work-framed-tn-01.jpg',alt="")
    li.work-item
      a.desc(href="img/work-framed-full-02.jpg",rel="group")
        .title You Don't Say
        .type Acrylic
        .dimension 24x24
      img(src='img/work-framed-tn-02.jpg',alt="")
                       
